{
  "name": "mithril-infinite",
  "main": "dist/mithril-infinite.js",
  "module": "dist/mithril-infinite.mjs",
  "version": "1.1.6",
  "description": "Infinite Scroll for Mithril",
  "scripts": {
    "build": "DEPS=false ../../node_modules/rollup/bin/rollup -c ../../scripts/rollup.umd.js && ../../node_modules/rollup/bin/rollup -c ../../scripts/rollup.es.js",
    "build-standalone": "DEPS=1 MODULE=infinite DEST=./dist/mithril-infinite-standalone.js ../../node_modules/rollup/bin/rollup -c ../../scripts/rollup.umd-includes.js"
  },
  "dependencies": {
    "j2c": "^1.0.0-4",
    "mithril": "^1.1.6",
    "resize-observer-polyfill": "^1.5.0",
    "verge": "^1.10.2"
  },
  "keywords": [
    "scroll",
    "infinite",
    "mithril",
    "occlusion culling"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ArthurClemens/mithril-infinite.git"
  },
  "author": "Arthur Clemens <arthurclemens@gmail.com> (http://visiblearea.com)",
  "license": "MIT"
}
